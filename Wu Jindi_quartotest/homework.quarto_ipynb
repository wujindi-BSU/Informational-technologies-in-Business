        "plt.figure(figsize=(10, 5))\n",
        "imdb_genres.head(10).plot(kind='bar', alpha=0.7, color='orange')\n",
        "plt.title('Top 10 IMDb Genres')\n",
        "plt.xlabel('Genre')\n",
        "plt.ylabel('Count')\n",
        "plt.show()\n",
        "\n",
