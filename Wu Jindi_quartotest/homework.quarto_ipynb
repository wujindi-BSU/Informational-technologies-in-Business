        "            data.append(bd.strip())\n",
        "\n",
        "            datalist.append(data)\n",
        "    return datalist\n",
        "\n",
        "\n",
        "# Function to save data to a CSV file\n",
        "def savedata(datalist, savepath):\n",
        "    headers = [\"电影详情链接\", \"图片链接\", \"影片中文名\", \"影片外国名\", \"评分\", \"评价数\", \"概况\", \"相关信息\"]\n",
