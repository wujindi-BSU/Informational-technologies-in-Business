        "\n",
        "\n",
        "# Regular expressions to extract information\n",
        "findLink = re.compile(r'<a href=\"(.*?)\">')  # detail link\n",
        "findImgSrc = re.compile(r'<img.*src=\"(.*?)\"', re.S)  # image link\n",
